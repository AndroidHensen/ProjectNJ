<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.handsome.projectnz.View.Home.MaterialHandling.StockOutputNewActivity">

    <include layout="@layout/include_common_header" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.handsome.projectnz.CustomView.MenuBar
                android:id="@+id/project_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:menu_name="项目" />

            <include layout="@layout/include_common_split_line_hor" />

            <com.handsome.projectnz.CustomView.MenuBar
                android:id="@+id/material_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:menu_name="物资名称" />

            <include layout="@layout/include_common_split_line_hor" />

            <com.handsome.projectnz.CustomView.MenuBar
                android:id="@+id/material_type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:menu_name="型号规格" />

            <include layout="@layout/include_common_split_line_hor" />

            <com.handsome.projectnz.CustomView.MenuBar
                android:id="@+id/material_output_count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:menu_comment="(存：??)"
                app:menu_name="出库数量" />

            <include layout="@layout/include_common_split_line_hor" />

            <com.handsome.projectnz.CustomView.MenuBar
                android:id="@+id/material_department"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:menu_name="单位" />

            <include layout="@layout/include_common_split_line_hor" />

            <com.handsome.projectnz.CustomView.MenuBar
                android:id="@+id/material_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:menu_name="日期" />

            <include layout="@layout/include_common_split_line_hor" />

            <com.handsome.projectnz.CustomView.MenuBar
                android:id="@+id/output_manager"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:menu_name="经办人" />

            <include layout="@layout/include_common_split_line_hor" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical">

                <com.handsome.projectnz.CustomView.MenuBar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:menu_content=" "
                    app:menu_ic_visible="false"
                    app:menu_name="出库凭证" />

                <ImageView
                    android:id="@+id/output_picture"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="16dp"
                    android:background="@drawable/announcement_news_pic_camera" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>


</LinearLayout>
