<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.handsome.projectnz.Fragment.Project.ProjectNewDataFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <com.handsome.projectnz.CustomView.ProjectBuilderView
                android:layout_width="match_parent"
                app:builder_name="业主单位"
                android:id="@+id/owner"
                android:layout_height="wrap_content"/>
            <com.handsome.projectnz.CustomView.ProjectBuilderView
                android:layout_width="match_parent"
                app:builder_name="设计单位"
                android:id="@+id/designer"
                android:layout_height="wrap_content"/>
            <com.handsome.projectnz.CustomView.ProjectBuilderView
                android:layout_width="match_parent"
                app:builder_name="监工单位"
                android:id="@+id/checker"
                android:layout_height="wrap_content"/>
            <com.handsome.projectnz.CustomView.ProjectBuilderView
                android:layout_width="match_parent"
                android:id="@+id/contract"
                app:builder_name="合同单位"
                android:layout_height="wrap_content"/>

        </LinearLayout>
    </ScrollView>


</LinearLayout>
